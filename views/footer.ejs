<script>function formatDuration(duration) {
    const parts = duration.split(":");
    return `${parts[0]} hr ${parts[1]} min ${parts[2]} sec`;
  }


  function validateForm() {
    var filter = document.getElementById("filter").value;

    if (filter === "") {
      alert("Please select a method first.");
      return false; // Prevent form submission
    }
    
    // Form will be submitted if filter is selected
    return true;
  }
  

  document.getElementById("filter").addEventListener("change", function() {
    var filter = this.value;
    var dateInputContainer = document.getElementById("dateInputContainer");
    var dateRangeInputContainer = document.getElementById("dateRangeInputContainer");

    if (filter === "getByDate") {
      dateRangeInputContainer.style.display = "none";
      dateInputContainer.style.display = "block";
      
    } else if (filter === "getByDateRange") {
      dateInputContainer.style.display = "none";
      dateRangeInputContainer.style.display = "block";
    } else {
      dateInputContainer.style.display = "none";
      dateRangeInputContainer.style.display = "none";
    }
  });
  </script>
